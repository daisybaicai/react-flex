{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@umijs/hooks": "^1.9.2",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.1.1",
    "antd": "^4.1.5",
    "lint-staged": "^10.0.7",
    "loadsh": "0.0.4",
    "lrz": "^4.9.40",
    "prettier": "^1.19.1",
    "react": "^16.13.1",
    "react-cropper": "^1.3.0",
    "react-dom": "^16.12.0",
    "react-monaco-editor": "^0.36.0",
    "umi": "^3.1.1",
    "yorkie": "^2.0.0"
  },
  "devDependencies": {
    "echarts": "^4.7.0"
  }
}
